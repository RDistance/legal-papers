{
  "name": "legal-papers",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "description": "The Project of Electron + Vue + Vite + Electron Forge",
  "author": "plx",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "serve": "electron .",
    "start": "concurrently \"npm run dev\" \"electron-forge start\"",
    "build-only": "vite build",
